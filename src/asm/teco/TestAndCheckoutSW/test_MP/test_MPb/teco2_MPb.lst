02102    2102     00141 0                DS       %00141         ; check #14 (79 * 97)
02103    2103     00117 0                DS       %00117         ; check #15 (97 * 79)
02104    2104     00177 0                DS       %00177         ; check #16 (107 * 127)
02105    2105     00153 0                DS       %00153         ; check #17 (127 * 107)
                          
                          ; A = upper product
                          MPchkA         EQU      *              
                          ; randomly selected checks
02106    2106     00000 1                DS       %00000         ; check #08 (7 * 17)
02107    2107     00000 1                DS       %00000         ; check #09 (17 * 7)
02110    2110     00000 1                DS       %00000         ; check #10 (29 * 41)
